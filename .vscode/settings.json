{
  "typescript.tsdk": "node_modules/typescript/lib",
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier",
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "files.exclude": {
    "**/.yarn": true
  },
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.eol": "\n",
  "typescript.enablePromptUseWorkspaceTsdk": true
}
